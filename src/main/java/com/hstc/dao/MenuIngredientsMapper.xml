<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hstc.dao.MenuIngredientsMapper">
    <select id="getMenuIngredientsListByMid" parameterType="int"
            resultType="com.hstc.pojo.MenuIngredients">
        select * from menu_ingredients where menu_id=#{menuId}
    </select>

</mapper>
