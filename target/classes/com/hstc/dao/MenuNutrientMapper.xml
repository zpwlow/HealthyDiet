<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hstc.dao.MenuNutrientMapper">
    <select id="getMenuNutrientListByMid" parameterType="int"
            resultType="com.hstc.pojo.MenuNutrient">
        select * from menu_nutrient
        where menu_id=#{menuId}
    </select>

</mapper>
